<script type="text/javascript">
    //Determine how much padding is needed for the nav.  1170px - window width
    $(document).ready(function () {

        var e = window,
            a = 'inner';
        if (!('innerWidth' in window)) {
            a = 'client';
            e = document.documentElement || document.body;
        }
        /*var width = jQuery(window).width();*/
        var width = e[a + 'Width'];
        var navTotal = width - 1185;
        var navPadding = navTotal / 2;

        $('.cd-secondary-nav').attr('style', 'padding: 48px ' + (navPadding) + 'px 48px 180px; margin-left: -180px;');
        $('.cd-secondary-nav.cd-secondary-search-nav').attr('style', 'padding: 48px ' + (navPadding) + 'px; margin-left: -180px;');
    });
</script>

